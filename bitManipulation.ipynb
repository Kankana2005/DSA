      "text/plain": [
       "'1101 '"
      ]
     },
     "execution_count": 2,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def convert2Binary(num:int)->str:\n",
    "    result = \" \"\n",
    "    while num>0:\n",
    "        if num%2 ==1:\n",
    "            result+=\"1\"\n",
    "        else:\n",
    "            result+= \"0\"\n",
    "        num = num//2\n",
    "    result = result[::-1]\n",
    "    return result\n",
    "\n",
    "convert2Binary(13)         "
   ]
  }
