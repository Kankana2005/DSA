   "source": [
    "def convert2Binary(num:int)->str:\n",
    "    result = \" \"\n",
    "    while num>0:\n",
    "        if num%2 ==1:\n",
    "            result+=\"1\"\n",
    "        else:\n",
    "            result+= \"0\"\n",
    "        num = num//2\n",
    "    result = result[::-1]\n",
    "    return result\n",
